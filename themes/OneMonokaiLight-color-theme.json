  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#9aa3b2"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.template"
      ],
      "settings": {
        "foreground": "#d6a116"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#b74ed9"
      }
    },
    {
      "name": "Embedded String Begin/End",
      "scope": [
        "string.embedded.begin",
        "string.embedded.end",
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#b74ed9"
      }
    },
    {
      "name": "Default Embedded Color",
      "scope": [
